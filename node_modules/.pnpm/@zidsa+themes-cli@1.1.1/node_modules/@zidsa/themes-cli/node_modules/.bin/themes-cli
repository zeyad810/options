#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/themes-cli/bin/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/themes-cli/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/themes-cli/bin/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/themes-cli/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules/@zidsa/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/@zidsa+themes-cli@1.1.1/node_modules:/home/rania/themiify_projects/zid_glamour/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/zid-theme" "$@"
else
  exec node  "$basedir/../../bin/zid-theme" "$@"
fi
